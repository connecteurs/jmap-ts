    "io-ts": "^2.2.5"
  },
  "devDependencies": {
    "fp-ts": "^2.6.5",
    "io-ts": "^2.2.5",
    "rimraf": "^3.0.2",
    "tsd": "^0.11.0",
    "typescript": "^3.9.5"
  },
  "files": [
    "./lib"
